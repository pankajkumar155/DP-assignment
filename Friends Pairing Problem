#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define ff first
#define ss second
#define pb push_back
#define endl "\n"
#define fast ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
ll mod=1e9+7;



	   
int main() {
			fast
			
			 
			ll t;
			cin>>t;
			while(t--)
			{
				ll n;
				cin>>n;
				ll dp[n+2];
				dp[0]=0;
				dp[1]=1;
				dp[2]=2;
				for(ll i=3;i<=n;i++)
				{
					dp[i]=dp[i-1];
					dp[i]+=(i-1)*dp[i-2]; 
				}
				cout<<dp[n]<<endl;
			}

			return 0;
		 }
